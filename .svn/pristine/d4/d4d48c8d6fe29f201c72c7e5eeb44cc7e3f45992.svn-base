
<HTML>
    
    <HEAD>
        <!--LINK REL="stylesheet" type="text/css" href="../megasoft.css" /-->
        <LINK REL="stylesheet" type="text/css" href="../css/HojadeEstilo.css"/>
        <TITLE>Sistema INFI - Seguridad</TITLE>
    </HEAD>
    
    <!--poner el focus en el 1er control de la forma-->
    <BODY ONLOAD="javascript:{document.forms[0].elements[0].focus();if(parent.frames[1]&&parent.frames['sidebar'].Go)parent.frames['sidebar'].Go()};">
        
        
<!--page name bar-->
<TABLE  border="0" cellspacing="0" WIDTH="100%">
    <TR>
        <TD class="headerBox">                
            <FONT class="headerText">Seguridad / Roles</FONT>
        </TD>
	   		<!-- help online -->
	   		<!--TD class="headerBox" valign="middle" align="right">
	   		<img border="0" src="@url_images@ayuda.gif" width="17" height="17"  onclick="helpOpen();" style="cursor:hand" title="Mostrar ayuda en l&iacute;nea">
	   		</TD-->
	   		<!-- fin help online -->
   		</TR>
</TABLE>
        
        <CENTER>

<!--formulario-->
<form NAME="dataentry" METHOD="POST" ACTION="msc_role-insert">
		
        <table border="0" cellspacing="1" cellpadding="2" width="45%" class="dataform">
        
        <!--titulo-->
        <tr CLASS="formCaption">
            <th COLSPAN="2" ALIGN="center">
                <FONT class="formCaptionText">
                 Nuevo Registro
                </FONT>
            </th>
        </tr>
        
        <!--campo-->
        <tr CLASS="formElement">
            <td CLASS="formLabel" width="50%">
             Rol (alias corto):*
            </td>
            <td>
            <INPUT TYPE="TEXT" VALUE="" NAME="rolename" SIZE="30" MAXLENGTH="50" class="inputControl" />
            </td>
        </tr>
                
        <!--campo-->
        <tr CLASS="formElement">
            <td CLASS="formLabel">
             Descripci&oacute;n:*
            </td>
            <td>
            <INPUT TYPE="TEXT" VALUE="" NAME="description" SIZE="30" MAXLENGTH="100" class="inputControl" />
            </td>
        </tr>
                


        </table>

            <br>
            <button TYPE="submit" />Grabar</button>
            <SPAN> </SPAN>
            <button ONCLICK="history.back();"/>Cancelar</button>

            <!--campo escondido para almacenar el key del record-->
            <INPUT TYPE="HIDDEN" VALUE="" NAME="id" />

    </form>

        
        </CENTER>
        
    </BODY>

</HTML>

