package com.bdv.infi.data;

import java.util.Date;

public class CuentasBancoBDV {

	/**
	 * Id de la cuenta
	 */
	long idCuenta;
	/**
	 * Codigo bic del banco
	 */
	String codigoBicBanco;
	/**
	 * Numero de Cuenta
	 */
	String numeroCuenta;
	/**
	 * Descripcion
	 */
	String descripcion;
	
	/**Fecha de última actualización*/
	Date fechaUltActualizacion;
	
	/**Usuario (NM,CT) de última actualización*/
	String usrUltActualizacion;
	
	/**
	 * Transaccion asociada
	 */
	String transaccion;
	public String getCodigoBicBanco() {
		return codigoBicBanco;
	}
	public void setCodigoBicBanco(String codigoBicBanco) {
		this.codigoBicBanco = codigoBicBanco;
	}
	public String getDescripcion() {
		return descripcion;
	}
	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}
	public long getIdCuenta() {
		return idCuenta;
	}
	public void setIdCuenta(long idCuenta) {
		this.idCuenta = idCuenta;
	}
	public String getNumeroCuenta() {
		return numeroCuenta;
	}
	public void setNumeroCuenta(String numeroCuenta) {
		this.numeroCuenta = numeroCuenta;
	}
	public String getTransaccion() {
		return transaccion;
	}
	public void setTransaccion(String transaccion) {
		this.transaccion = transaccion;
	}
	public Date getFechaUltActualizacion() {
		return fechaUltActualizacion;
	}
	public void setFechaUltActualizacion(Date fechaUltActualizacion) {
		this.fechaUltActualizacion = fechaUltActualizacion;
	}
	public String getUsrUltActualizacion() {
		return usrUltActualizacion;
	}
	public void setUsrUltActualizacion(String usrUltActualizacion) {
		this.usrUltActualizacion = usrUltActualizacion;
	}
	
	
}
