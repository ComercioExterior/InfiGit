package com.bdv.infi.data;

import java.io.Serializable;
import java.util.Date;

public class OperacionIntento implements Serializable {

	/**
	 * 
	 */
	private static final long serialVersionUID = -1438370757813061428L;
	/** 
	 * Id de la operación generada 
	 **/
	private long idOperacion = 0;
	/** 
	 * Id de la Orden
	 **/
	private long idOrden = 0;
	/**
	 * Id del Intento de aplicar la operacion 
	 */
	private long idOperacionIntento = 0;
	/**
	 * Fecha en la cual se ejecuta el intento
	 */
	private Date fechaAplicacion;		
	/** 
	 * Indicador si la operacion fue aplicada o no
	 **/
	private int indicadorAplicado = 0;
	/**
	 * Texto de Error reportado por el servicio
	 */
	private String textoError = "";

	/**
	 * Retorna el valor del Id de la OrdenOperacion 
	 * @return idOperacion
	 */
	public long getIdOperacion() {
		return idOperacion;
	}
	/**
	 * Asigna valor al Id de la OrdenOperacion 
	 * @param idOperacion
	 */	
	public void setIdOperacion(long idOperacion) {
		this.idOperacion = idOperacion;
	}
	
	/**
	 * Devuelve el valor del Identificador de la Orden 
	 * @return idOrden
	 */
	public long getIdOrden() {
		return idOrden;
	}
	/**
	 * Asigna valor al Identificador de la Orden 
	 * @param idOrden
	 */
	public void setIdOrden(long idOrden) {
		this.idOrden = idOrden;
	}
	
	/**
	 * Retorna el valor del Id del Intento de aplicar la operacion
	 * @return idOperacionIntento
	 */
	public long getIdOperacionIntento() {
		return idOperacionIntento;
	}
	/**
	 * Asigna valor al Id del Intento de aplicar la operacion
	 * @param idOperacionIntento
	 */	
	public void setIdOperacionIntento(long idOperacionIntento) {
		this.idOperacionIntento = idOperacionIntento;
	}
	
	/**
	 * Devuelve el valor de la Fecha en la cual se ejecuta el intento
	 * @return fechaAplicacion
	 */
	public Date getFechaAplicacion() {
		return fechaAplicacion;
	}
	/**
	 * Asigna valor a la Fecha en la cual se ejecuta el intento
	 * @param fechaAplicacion
	 */
	public void setFechaAplicacion(Date fechaAplicacion) {
		this.fechaAplicacion = fechaAplicacion;
	}

	/**
	 * Retorna el valor del Indicador si la operacion fue aplicada o no  
	 * @return indicadorAplicado
	 */
	public int getIndicadorAplicado() {
		return this.indicadorAplicado;
	}
	/**
	 * Asigna valor al Indicador si la operacion fue aplicada o no
	 * @param indicadorAplicado
	 */
	public void setIndicadorAplicado(int indicadorAplicado) {
		this.indicadorAplicado = indicadorAplicado;
	}
	
	/**
	 * Retorna el valor del Texto de Error reportado por el servicio
	 * @return textoError
	 */
	public String getTextoError() {
		return textoError;
	}
	/**
	 * Asigna valor al Texto de Error reportado por el servicio
	 * @param textoError
	 */
	public void setTextoError(String textoError) {
		this.textoError = textoError;
	}
}

