	<queryString><![CDATA[select 
  a.titulo_id, a.tipo_producto_id,a.titcus_cantidad-FUNC_INFI_CANTIDAD_BLOQUEADA(a.client_id,a.titulo_id,a.tipo_producto_id,'$P!{p_fecha_cierre_sf}') titcus_cantidad, (b.tipper_id || '-' || b.client_cedrif) client_cedrif,b.client_cta_custod_id,b.client_id,b.client_nombre,decode(c.titulos_precio_recompra,null,0,c.titulos_precio_recompra) titulos_precio_recompra, 0 cantidad_bloqueada, 'DISPONIBLE' estados,a.tasa_cambio TCC_TASA_CAMBIO_COMPRA, ((a.titcus_cantidad-FUNC_INFI_CANTIDAD_BLOQUEADA(a.client_id,a.titulo_id,a.tipo_producto_id,'$P!{p_fecha_cierre_sf}')) * a.tasa_cambio) total,((a.titcus_cantidad-FUNC_INFI_CANTIDAD_BLOQUEADA(a.client_id,a.titulo_id,a.tipo_producto_id,'$P!{p_fecha_cierre_sf}')) * decode(c.titulos_precio_recompra,null,0,c.titulos_precio_recompra) /100) as total_precio,e.mdate titulo_fe_vencimiento, e.ccy titulo_moneda_den
from 
  infi_tb_702_titulos_cierre a, 
  infi_tb_201_ctes b,
  (select titulo_id, tipo_producto_id,titulos_precio_recompra from infi_tb_119_titulos_prec_hist where fecha_aprobacion in(
    select max(fecha_aprobacion) from infi_tb_119_titulos_prec_hist 
      where trunc(fecha_aprobacion) <= $P!{p_fechaCierre} 
  )) c,
  ( select client_id,titulo_id,tipo_producto_id,max(fecha_cierre) as fecha_cierre from infi_tb_702_titulos_cierre 
   where trunc(fecha_cierre) <=   $P!{p_fechaCierre}
   group by client_id,titulo_id,tipo_producto_id
  ) d, secm e
where a.client_id = b.client_id and a.titulo_id = c.titulo_id(+) 
and a.tipo_producto_id = c.tipo_producto_id(+) and a.tipo_producto_id = c.tipo_producto_id(+) 
and a.client_id = d.client_id and a.titulo_id = d.titulo_id and a.tipo_producto_id = d.tipo_producto_id and a.fecha_cierre = d.fecha_cierre
$P!{p_cliente}
$P!{p_titulo}
$P!{p_tipoProducto}
and trunc(a.fecha_cierre) <= $P!{p_fechaCierre}
and trim(a.titulo_id) = trim(e.secid)
union


select
  bloq.titulo_id, bloq.tipo_producto_id,bloq.titcus_cantidad,(b.tipper_id || '-' || b.client_cedrif) client_cedrif,b.client_cta_custod_id,b.client_id,b.client_nombre,0 precio_recompra,0 cantidad_bloqueada, tipbloq.tipblo_descripcion estados,bloq.tasa_cambio, (bloq.titcus_cantidad*bloq.tasa_cambio) total, 0 total_precio,e.mdate titulo_fe_vencimiento,e.ccy titulo_moneda_den 
from 
  infi_tb_702_titulos_cierre a, 
  infi_tb_201_ctes b,  
  ( select client_id,titulo_id,tipo_producto_id,max(fecha_cierre) as fecha_cierre from infi_tb_702_titulos_cierre 
   where trunc(fecha_cierre) <=   $P!{p_fechaCierre}
   group by client_id,titulo_id,tipo_producto_id
  ) d, 
  (select client_id,titulo_id,tipo_producto_id,max(fecha) as fecha from infi_tb_705_titulos_bloq_hist 
   where trunc(fecha) <=   $P!{p_fechaCierre}
   group by client_id,titulo_id,tipo_producto_id
   ) bloqhist, secm e, infi_tb_705_titulos_bloq_hist bloq, infi_tb_700_tipo_bloqueo tipbloq
where a.client_id = b.client_id  
and a.client_id = d.client_id and a.titulo_id = d.titulo_id and a.tipo_producto_id = d.tipo_producto_id and a.fecha_cierre = d.fecha_cierre
and a.client_id = bloqhist.client_id and a.titulo_id = bloqhist.titulo_id and a.tipo_producto_id = bloqhist.tipo_producto_id
and bloqhist.client_id = bloq.client_id and bloqhist.titulo_id = bloq.titulo_id and bloqhist.tipo_producto_id = bloq.tipo_producto_id and bloqhist.fecha = bloq.fecha and bloq.titcus_cantidad > 0
and bloq.tipblo_id  = tipbloq.tipblo_id 
$P!{p_cliente}
$P!{p_titulo}
$P!{p_tipoProducto}
and trunc(a.fecha_cierre) <= $P!{p_fechaCierre}
and trim(a.titulo_id) = trim(e.secid)
order by tipo_producto_id,client_id,titulo_moneda_den,estados desc]]></queryString>

