
package com.bdv.infi.data;
import java.util.Date;

/**
 * @author Megasoft Computacion
 *
 */
public class EquivalenciaPortafolio {
	
	//Id del segmento
	private String IdSegmento;
	
	//descripcion del segmento
	private String DescripcionSegmento;
	
	//id del portafolio enviar a OPICS
	private String Portafolio;
	
	//Id del Usuario que modifica el registro
	private int IdUsuario;
	
	//Fecha de la ultima actualizacion del registro
	private Date FechaActualizacion;

	//Id del Segmento Anterior
	private String IdSegmentoAnterior;
	
	
	/**
	 * @return the idSegmentoAnterior
	 */
	public String getIdSegmentoAnterior() {
		return IdSegmentoAnterior;
	}

	/**
	 * @param idSegmentoAnterior the idSegmentoAnterior to set
	 */
	public void setIdSegmentoAnterior(String idSegmentoAnterior) {
		IdSegmentoAnterior = idSegmentoAnterior;
	}

	/**
	 * @return the idSegmento
	 */
	public String getIdSegmento() {
		return IdSegmento;
	}

	/**
	 * @param idSegmento the idSegmento to set
	 */
	public void setIdSegmento(String idSegmento) {
		IdSegmento = idSegmento;
	}

	/**
	 * @return the descripcionSegmento
	 */
	public String getDescripcionSegmento() {
		return DescripcionSegmento;
	}

	/**
	 * @param descripcionSegmento the descripcionSegmento to set
	 */
	public void setDescripcionSegmento(String descripcionSegmento) {
		DescripcionSegmento = descripcionSegmento;
	}

	/**
	 * @return the portafolio
	 */
	public String getPortafolio() {
		return Portafolio;
	}

	/**
	 * @param portafolio the portafolio to set
	 */
	public void setPortafolio(String portafolio) {
		Portafolio = portafolio;
	}

	/**
	 * @return the idUsuario
	 */
	public int getIdUsuario() {
		return IdUsuario;
	}

	/**
	 * @param idUsuario the idUsuario to set
	 */
	public void setIdUsuario(int idUsuario) {
		IdUsuario = idUsuario;
	}

	/**
	 * @return the fechaActualizacion
	 */
	public Date getFechaActualizacion() {
		return FechaActualizacion;
	}

	/**
	 * @param fechaActualizacion the fechaActualizacion to set
	 */
	public void setFechaActualizacion(Date fechaActualizacion) {
		FechaActualizacion = fechaActualizacion;
	}
	
	
	
}
