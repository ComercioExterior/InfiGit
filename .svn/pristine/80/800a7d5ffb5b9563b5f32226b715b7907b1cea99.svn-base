package com.bdv.infi.data;

/**
 * Datos relacionados a las reglas que deben validarse cuando el cliente desea adquirir divisas
 */
public class ReglaConcepto {
	private int concepto_id;
	private int reglaValidacionId;
	private int reglaConceptoId;
	public int getConcepto_id() {
		return concepto_id;
	}
	public void setConcepto_id(int concepto_id) {
		this.concepto_id = concepto_id;
	}
	public int getReglaValidacionId() {
		return reglaValidacionId;
	}
	public void setReglaValidacionId(int reglaValidacionId) {
		this.reglaValidacionId = reglaValidacionId;
	}
	public int getReglaConceptoId() {
		return reglaConceptoId;
	}
	public void setReglaConceptoId(int reglaConceptoId) {
		this.reglaConceptoId = reglaConceptoId;
	}
	
	
}
