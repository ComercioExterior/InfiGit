package com.bdv.infi.data;

/** 
 * Clase que representa la entidad moneda
 */
public class Moneda {

/**
 * Id de la moneda
 */
private String id;

/**
 * Descripci&oacute;n de la moneda
 */
private String descripcion;

/**Indica el status de moneda 1=activo 0=inactivo*/
private boolean status;

/**Indica si es la moneda local*/
private boolean monedaLocal;

/**Indica si es la moneda de cambio*/
private boolean monedaCambio;

/**Descripci&oacute;n de las siglas de la moneda*/
private String siglas;


/**
 * Getter of the property <tt>descripcion</tt>
 *
 * @return Returns the descripcion.
 * 
 */
public String getDescripcion()
{
	return descripcion;
}

/**
 * Setter of the property <tt>descripcion</tt>
 *
 * @param descripcion The descripcion to set.
 *
 */
public void setDescripcion(String descripcion ){
	this.descripcion = descripcion;
}


 
/**
 * Getter of the property <tt>id</tt>
 *
 * @return Returns the id.
 * 
 */
public String getId()
{
	return id;
}

/**
 * Setter of the property <tt>id</tt>
 *
 * @param id The id to set.
 *
 */
public void setId(String id ){
	this.id = id;
}

public boolean isStatus() {
	return status;
}

public void setStatus(boolean status) {
	this.status = status;
}

public boolean isMonedaLocal() {
	return monedaLocal;
}

public void setMonedaLocal(boolean monedaLocal) {
	this.monedaLocal = monedaLocal;
}

public boolean isMonedaCambio() {
	return monedaCambio;
}

public void setMonedaCambio(boolean monedaCambio) {
	this.monedaCambio = monedaCambio;
}

public String getSiglas() {
	return siglas;
}

public void setSiglas(String siglas) {
	this.siglas = siglas;
}

}
