<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
-->
        
        <field 
        name="client_id" 
        type="integer" 
        description="Cliente id"
        nullable="1"
        size="7" />   
        
         <field 
        name="status" 
        type="varchar" 
        description="status cruce"
        nullable="1"
        size="50" />
        
        <field 
        name="statusp" 
        type="varchar" 
        description="Cruce Procesado"
        nullable="1"
        size="50" />
        
        <field 
        name="statusE" 
        type="varchar" 
        description="estatus envio"
        nullable="1"
        size="50" />
        
        <field 
        name="Tipo" 
        type="varchar" 
        description="tipo de movimiento"
        nullable="1"
        size="50" />
        
         <field 
        name="fecha" 
        type="varchar" 
        description="Fecha en la que se va a realizar la consulta"
        nullable="1"
        size="50" />
    
</record>

