<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="undinv_id" 
        type="integer" 
        description="Unidad Inversion"
        nullable="0"
        size="7" />
        
        <field 
        name="fecha" 
        type="varchar" 
        description="Fecha"
        nullable="0"
        size="10" /> 
        
        <field 
        name="estatus_solicitud" 
        type="varchar" 
        description="Estatus de la solicitud"
        nullable="0"
        size="50" /> 
        
        <field 
        name="tipo_solicitud" 
        type="varchar" 
        description="Tipo de solicitud (Oferta/Demanda)"
        nullable="0"
        size="50" /> 
        
</record>

