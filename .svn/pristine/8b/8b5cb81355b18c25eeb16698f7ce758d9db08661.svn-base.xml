<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
        <field 
        name="directorio" 
        type="varchar" 
        description="Directorio"
        nullable="1"
        size="500" />
               
        <field 
        name="filename" 
        type="varchar" 
        description="Logger"
        nullable="1"
        size="50" />
        
        <field 
        name="logger" 
        type="varchar" 
        description="Logger"
        nullable="1"
        size="50" />
		
		<field 
        name="prev" 
        type="integer" 
        description="Previous Page"
        nullable="1"
        size="50" />
     
		<field 
        name="next" 
        type="integer" 
        description="Next Page"
        nullable="1"
        size="50" />

    
</record>

