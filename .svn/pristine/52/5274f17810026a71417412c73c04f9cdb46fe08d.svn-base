package com.bdv.infi_services.beans.entidades;

/**
 * Lista de String que son parametros para la consulta de Unidades de Inversion
 * @author MegaSoft Computacion
 */
public class ParametrosPaginacion {
	
	/**
	 * Indicador de aplicar proceso de paginacion a la consulta
	 */
	private String indicaPagineo = "false";
	/**
	 * Numero de la pagina a recuperar
	 */
	private String paginaRequerida = "0";
	/**
	 * Cantidad de registros que debe recuperar una pagina 
	 */
	private String registrosPagina = "14";
	/**
	 * Cantidad de paginas generadas por los criterios
	 */
	private String cantPaginas = "0";
	
	/**
	 * Devuelve el valor del Indicador de aplicar proceso de paginacion a la consulta
	 * @return indicaPagineo
	 */
	public String getIndicaPagineo() {
		return indicaPagineo;
	}
	/**
	 * Asigna el valor al Indicador de aplicar proceso de paginacion a la consulta
	 * @param indicaPagineo
	 */
	public void setIndicaPagineo(String indicaPagineo) {
		this.indicaPagineo = indicaPagineo;
	}
	
	/**
	 * Devuelve el valor del Numero de la pagina a recuperar
	 * @return paginaRequerida
	 */
	public String getPaginaRequerida() {
		return paginaRequerida;
	}
	/**
	 * Asigna el valor al Numero de la pagina a recuperar
	 * @param paginaRequerida
	 */
	public void setPaginaRequerida(String paginaRequerida) {
		this.paginaRequerida = paginaRequerida;
	}
	
	/**
	 * Devuelve el valor del Cantidad de registros que debe recuperar una pagina
	 * @return registrosPagina
	 */
	public String getRegistrosPagina() {
		return registrosPagina;
	}
	/**
	 * Asigna el valor a la Cantidad de registros que debe recuperar una pagina
	 * @param registrosPagina
	 */
	public void setRegistrosPagina(String registrosPagina) {
		this.registrosPagina = registrosPagina;
	}
	
	/**
	 * Devuelve el valor de la Cantidad de paginas generadas por los criterios
	 * @return cantPaginas
	 */
	public String getCantPaginas() {
		return cantPaginas;
	}
	/**
	 * Asigna valor a la Cantidad de paginas generadas por los criterios
	 * @param cantPaginas
	 */
	public void setCantPaginas(String cantPaginas) {
		this.cantPaginas = cantPaginas;
	}

}
