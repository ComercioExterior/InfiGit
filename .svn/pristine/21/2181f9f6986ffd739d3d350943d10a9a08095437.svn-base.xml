<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="empres_nombre" 
        type="varchar" 
        description="Nombre "
        nullable="1"
        size="50" />
        
        <field 
        name="empres_rif" 
        type="varchar" 
        description="RIF"
        nullable="1"
        size="11" />
        
        <field 
        name="empres_status" 
        type="integer" 
        description="Estatus"
        nullable="1"
        size="1" />
        <field 
        name="tipper_id" 
        type="varchar" 
        description=""
        nullable="1"
        size="1" />
    
           
    
</record>

