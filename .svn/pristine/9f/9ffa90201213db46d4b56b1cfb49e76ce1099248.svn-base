package com.bdv.infi.data;

import java.math.BigDecimal;
import java.util.ArrayList;
import java.util.Date;

/** 
 * Clase que representa la informaci&oacute;n principal en el proceso de una generaci&oacute;n de archivos
 */
public class ConfiguracionTasa {
	
	private String idTasa;
	
	private String nmUsuarioCreador;
	private String nmUsuarioMofidicador;
	private String nmUsuarioAprobador;
	
	private String fechaTasa;
	private BigDecimal tasa;
	
	private String tipoProducto;
	private String transaccionId;
	
	private boolean aprobado=false;
	
	public String getFechaTasa() {
		return fechaTasa;
	}
	public void setFechaTasa(String fechaTasa) {
		this.fechaTasa = fechaTasa;
	}
	public String getNmUsuarioAprobador() {
		return nmUsuarioAprobador;
	}
	public void setNmUsuarioAprobador(String nmUsuarioAprobador) {
		this.nmUsuarioAprobador = nmUsuarioAprobador;
	}
	public String getNmUsuarioCreador() {
		return nmUsuarioCreador;
	}
	public void setNmUsuarioCreador(String nmUsuarioCreador) {
		this.nmUsuarioCreador = nmUsuarioCreador;
	}
	public BigDecimal getTasa() {
		return tasa;
	}
	public void setTasa(BigDecimal tasa) {
		this.tasa = tasa;
	}
	public String getTipoProducto() {
		return tipoProducto;
	}
	public void setTipoProducto(String tipoProducto) {
		this.tipoProducto = tipoProducto;
	}
	public String getTransaccionId() {
		return transaccionId;
	}
	public void setTransaccionId(String transaccionId) {
		this.transaccionId = transaccionId;
	}
	public boolean isAprobado() {
		return aprobado;
	}
	public void setAprobado(boolean aprobado) {
		this.aprobado = aprobado;
	}
	public String getIdTasa() {
		return idTasa;
	}
	public void setIdTasa(String idTasa) {
		this.idTasa = idTasa;
	}
	public String getNmUsuarioMofidicador() {
		return nmUsuarioMofidicador;
	}
	public void setNmUsuarioMofidicador(String nmUsuarioMofidicador) {
		this.nmUsuarioMofidicador = nmUsuarioMofidicador;
	}
	
	

	
}
