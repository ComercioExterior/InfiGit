
<HTML>
    
    <HEAD>
    <LINK REL="stylesheet" type="text/css" href="../js/calendar/popcalendar.css"/> 
            <SCRIPT language='javascript' src="../js/calendar/popcalendar.js"></SCRIPT>
           <script language="javascript" src="../js/default.js"></script>
           <script language="javascript" src="../js/cierres.js"></script>
              
        <LINK REL="stylesheet" type="text/css" href="../css/HojadeEstilo.css"/>
        
        <TITLE>Sistema INFI</TITLE>
        <style type="text/css">
          .dataTable{
            table-layout: auto !important; 
          }
        </style>
         
    </HEAD>
    
<BODY>
      
        
<!--page name bar-->
<TABLE  border="0" cellspacing="0" WIDTH="150%">
    <TR>
        <TD class="headerBox">                
            <FONT class="headerText">Utilitarios / Consulta de Procesos</FONT>
        </TD>
        
        <!-- help online -->
   		<TD class="headerBox" valign="middle" align="right">
   			<img border="0" src="../images/infi_gn_ayuda.gif" width="17" height="17" onclick="helpOpen(window.document.location.href);" style="cursor:hand" title="Mostrar ayuda en l&iacute;nea" >
   		</TD>
   		<!-- fin help online -->
        
        <td class="headerBox" align="right"></td>
</TABLE>
        
        <CENTER>

        <table border="0" cellspacing="1" cellpadding="2" width="150%" class="dataTable">
        <tr>
            <th colspan="7" align="center">                
                  Lista de Procesos              
            </th>
        </tr>
        <tr>
	        <th>Id</th>
	        <th>Transacción</th>
	        <th>Fecha Inicio</th>
	        <th>Fecha Fin</th>
	        <th>Error</th>
	        <th>Usuario</th>
	        <th>Acción</th>
        </tr>
        <procesos>
        <tr>
            <td align="right">@ejecucion_id format-mask='######'@</td>
            <td align="left">@transa_id@</td>
            <td align="center">@fecha_inicio format-mask='dd-MM-yyyy hh:mm:ss a'@</td>
            <td align="center">@fecha_fin format-mask='dd-MM-yyyy hh:mm:ss a'@</td>
            <td>@desc_error@</td>
            <td>@userid@</td>
            <td align="center" >@accion@</td>            
        </tr>
        </procesos>
        </table>
        @page_index@
        <BR>
        <INPUT TYPE="HIDDEN" VALUE="true" NAME="pagestart"/> 	
        <br/>
        <button  ONCLICK="history.back()">Regresar</button>&nbsp;
        <button type="button" ONCLICK="window.location='show-view'">&nbsp;Salir&nbsp;</button>
        </CENTER>
    </BODY>
</HTML>

