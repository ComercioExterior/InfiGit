package com.bdv.infi_services.beans.entidades.mensajes_respuesta;

import java.io.Serializable;
import java.util.ArrayList;

public class OperacionesFinancieras implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;
	/**
	 * Array de operaciones
	 */
	public ArrayList<OperacionFinanciera> operaciones;

	/**
	 * @return the operaciones
	 */
	public ArrayList<OperacionFinanciera> getOperaciones() {
		return operaciones;
	}
	/**
	 * @param operaciones the operaciones to set
	 */
	public void setOperaciones(ArrayList<OperacionFinanciera> operaciones) {
		this.operaciones = operaciones;
	}
}
