package com.bdv.infi.webservices.beans.consult;

import com.bdv.infi.webservices.beans.CredencialesDeUsuario;

/**
 * Utilizado para realizar consultas por cedula de identidad del cliente
 * 
 * @author cet
 * 
 */
public class ConsultaPorCI {
	/**
	 * usuario y pasword
	 */
	private CredencialesDeUsuario credenciales;
	
	/**
	 * numero de cedula del cliente
	 */
	private String ci;
	private String entidad;
	private String numeroPersona;
	private String secuenciaDocumento;
	private String indicadorGrupo;
	private String nombreFantasia;
	private String primerApellido;
	private String segundoApellido;
	public String getCi() {
		return ci;
	}
	public void setCi(String ci) {
		this.ci = ci;
	}
	public CredencialesDeUsuario getCredenciales() {
		return credenciales;
	}
	public void setCredenciales(CredencialesDeUsuario credenciales) {
		this.credenciales = credenciales;
	}
	public String getEntidad() {
		return entidad;
	}
	public void setEntidad(String entidad) {
		this.entidad = entidad;
	}
	public String getIndicadorGrupo() {
		return indicadorGrupo;
	}
	public void setIndicadorGrupo(String indicadorGrupo) {
		this.indicadorGrupo = indicadorGrupo;
	}
	public String getNombreFantasia() {
		return nombreFantasia;
	}
	public void setNombreFantasia(String nombreFantasia) {
		this.nombreFantasia = nombreFantasia;
	}
	public String getNumeroPersona() {
		return numeroPersona;
	}
	public void setNumeroPersona(String numeroPersona) {
		this.numeroPersona = numeroPersona;
	}
	public String getPrimerApellido() {
		return primerApellido;
	}
	public void setPrimerApellido(String primerApellido) {
		this.primerApellido = primerApellido;
	}
	public String getSecuenciaDocumento() {
		return secuenciaDocumento;
	}
	public void setSecuenciaDocumento(String secuenciaDocumento) {
		this.secuenciaDocumento = secuenciaDocumento;
	}
	public String getSegundoApellido() {
		return segundoApellido;
	}
	public void setSegundoApellido(String segundoApellido) {
		this.segundoApellido = segundoApellido;
	}
}
