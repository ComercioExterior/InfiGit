package com.bdv.infi_services.beans.entidades.mensajes_respuesta;

import java.io.Serializable;

public class Documento implements Serializable{

	/**
	 * 
	 */
	private static final long serialVersionUID = 1L;

	/**
	 * Id del documento asociado a la orden
	 */
	private String ordenDocumentoId;
	
	/**
	 * Contenido del documento en base64
	 */
	private String contenido;
	/**
	 * Nombre del documento
	 */
	private String nombreDocumento;
	/**
	 * @return the nombreDocumento
	 */
	public String getNombreDocumento() {
		return nombreDocumento;
	}
	/**
	 * @param nombreDocumento the nombreDocumento to set
	 */
	public void setNombreDocumento(String nombreDocumento) {
		this.nombreDocumento = nombreDocumento;
	}
	/**
	 * @return the ordenDocumentoId
	 */
	public String getOrdenDocumentoId() {
		return ordenDocumentoId;
	}
	/**
	 * @param ordenDocumentoId the ordenDocumentoId to set
	 */
	public void setOrdenDocumentoId(String ordenDocumentoId) {
		this.ordenDocumentoId = ordenDocumentoId;
	}
	public String getContenido() {
		return contenido;
	}
	public void setContenido(String contenido) {
		this.contenido = contenido;
	}
}
