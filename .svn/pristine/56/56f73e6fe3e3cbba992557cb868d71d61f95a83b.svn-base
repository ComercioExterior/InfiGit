package com.bdv.infi.data;

import java.util.Date;


/**Representa un registro de la tabla INFI_TB_702_TITULOS_CIERRE*/
public class TitulosCierre {
	
	/**Id del título del cliente*/
	private String idTitulo;
	
	/**Id del cliente*/
	private long idCliente;
	
	/**Fecha de cierre*/
	private Date fechaCierre;
	
	/**Cantidad o valor nominal que posee el cliente al cierre de un mes*/
	private long valorNominal;

	
	/**Obtiene el id del título*/
	public String getIdTitulo() {
		return idTitulo;
	}

	/**Setea el id del título*/
	public void setIdTitulo(String idTitulo) {
		this.idTitulo = idTitulo;
	}

	/**Obtiene el id del cliente*/
	public long getIdCliente() {
		return idCliente;
	}

	/**Setea el id del cliente*/
	public void setIdCliente(long idCliente) {
		this.idCliente = idCliente;
	}

	/**Obtiene la fecha de cierre*/
	public Date getFechaCierre() {
		return fechaCierre;
	}

	/**Establece la fecha de cierre del mes*/
	public void setFechaCierre(Date fechaCierre) {
		this.fechaCierre = fechaCierre;
	}

	/**Obtiene el valor nominal del cliente con respecto al título*/
	public long getValorNominal() {
		return valorNominal;
	}

	/**Establece el valor nominal del cliente*/
	public void setValorNominal(long valorNominal) {
		this.valorNominal = valorNominal;
	}
	
}
