	private String nombreBeneficiario = null;
	
	private String descripcionBeneficiario = null;

	/**
	 * Retorna la descripci&oacute;n del beneficiario
	 * @return descripcionBeneficiario
	 */
	public String getDescripcionBeneficiario() {
		return descripcionBeneficiario;
	}

	/**
	 * Asignar descripci&oacute;n del beneficiario
	 * @param descripcionBeneficiario
	 */
	public void setDescripcionBeneficiario(String descripcionBeneficiario) {
