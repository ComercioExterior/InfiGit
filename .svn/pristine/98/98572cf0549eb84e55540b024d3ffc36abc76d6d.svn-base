	}

	/**
	 * Asignar nombre del beneficiario
	 * @param nombreBeneficiario
	 */
	public void setNombreBeneficiario(String nombreBeneficiario) {
		this.nombreBeneficiario = nombreBeneficiario;
	}
	 
	
	
}
