<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="idUnidadInversion" 
        type="integer" 
        description="Identificador de la Unidad de Inversion"
        nullable="1"
        size="10" />    
    
        <field 
        name="idEmpresa" 
        type="varchar" 
        description="Identificador de la Empresa"
        nullable="1"
        size="100" />
    
        <field 
        name="idRoles" 
        type="varchar" 
        description="Rol de la Empresa"
        nullable="0"
        size="100" />
        
        <field 
        name="nombreContacto" 
        type="varchar" 
        description="Nombre del Contacto"
        nullable="0"
        size="50" /> 
                
        <field 
        name="telefonoContacto" 
        type="varchar" 
        description="Telefono del Contacto"
        nullable="0"
        size="50" /> 
            
</record>

