package com.bdv.infi.data;

public class InstrumentoFinanciero {

	/**
	 * campo insfin_id
	 */
	private String idInstrumento;
	
	/**
	 * 	campo insfin_descripcion
	 */
	private String descripcion;
	
	/**
	 * 	campo insfin_forma_orden
	 */
	private String formaOrden;
	
	/**
	 * 	campo nsfin_metodo_cupones
	 */
	private String metodosCupones;
	
	/**
	 * 	campo insfin_multiples_ordenes
	 */
	private int multiplesCupones;
	
	/**
	 * 	campo insfin_in_inventario
	 */
	private int Inventario;
	
	/**
	 * 	campo MANEJO_PRODUCTOS
	 */
	private long manejoProducto=0;

	public String getDescripcion() {
		return descripcion;
	}
	
	/**
	 * Tipo de Producto del Instrumento
	 */
	private String tipoProductoId;


	public void setDescripcion(String descripcion) {
		this.descripcion = descripcion;
	}

	public String getFormaOrden() {
		return formaOrden;
	}

	public void setFormaOrden(String formaOrden) {
		this.formaOrden = formaOrden;
	}

	public String getIdInstrumento() {
		return idInstrumento;
	}

	public void setIdInstrumento(String idInstrumento) {
		this.idInstrumento = idInstrumento;
	}

	public int getInventario() {
		return Inventario;
	}

	public void setInventario(int inventario) {
		Inventario = inventario;
	}

	public String getMetodosCupones() {
		return metodosCupones;
	}

	public void setMetodosCupones(String metodosCupones) {
		this.metodosCupones = metodosCupones;
	}

	public int getMultiplesCupones() {
		return multiplesCupones;
	}

	public void setMultiplesCupones(int multiplesCupones) {
		this.multiplesCupones = multiplesCupones;
	}
	
	public String getTipoProductoId() {
		return tipoProductoId;
	}

	public void setTipoProductoId(String tipoProductoId) {
		this.tipoProductoId = tipoProductoId;
	}

	public long getManejoProducto() {
		return manejoProducto;
	}

	public void setManejoProducto(long manejoProducto) {
		this.manejoProducto = manejoProducto;
	}

	
}
