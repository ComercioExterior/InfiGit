<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->    
 
 
 
         <field 
         name="tipo_reprocesamiento" 
         type="varchar" 
         description="tipo de operacion de reprocesamiento"
         nullable="0"
         size="1" />

         <field 
         name="estatus_cierre" 
         type="varchar" 
         description="Estatus del proceso de cierre"
         nullable="0"
         size="1" />    
         
         <field 
         name="paso_ejecucion" 
         type="varchar" 
         description="Paso del cierre en el que se quedo la ejecucion"
         nullable="0"
         size="3" />   
         

</record>

