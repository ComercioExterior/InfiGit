	<queryString><![CDATA[select infi_tb_204_ordenes.ordene_id,infi_tb_207_ordenes_operacion.MONTO_OPERACION, 
	   infi_tb_207_ordenes_operacion.FECHA_PROCESADA,
	   infi_tb_207_ordenes_operacion.operacion_nombre as trnfin_nombre,
	   INFI_TB_102_BLOTER.BLOTER_DESCRIPCION,
	   (select sum(infi_tb_207_ordenes_operacion.MONTO_OPERACION)
	   from infi_tb_207_ordenes_operacion where infi_tb_207_ordenes_operacion.ORDENE_ID=infi_tb_204_ordenes.ordene_id)
	   as total
from infi_tb_204_ordenes
left join infi_tb_207_ordenes_operacion on infi_tb_204_ordenes.ORDENE_ID=infi_tb_207_ordenes_operacion.ordene_id
left join INFI_TB_102_BLOTER on infi_tb_204_ordenes.BLOTER_ID=INFI_TB_102_BLOTER.BLOTER_ID
where 1=1  $P!{p_filter}
and infi_tb_207_ordenes_operacion.STATUS_OPERACION='APLICADA'  and infi_tb_207_ordenes_operacion.TRNF_TIPO='DEB'
 and infi_tb_204_ordenes.ordsta_id='ADJUDICADA'
order by infi_tb_204_ordenes.ORDENE_ID desc]]></queryString>

