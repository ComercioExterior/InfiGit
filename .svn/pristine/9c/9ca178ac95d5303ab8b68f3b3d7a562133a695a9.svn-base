package com.bdv.infi.data;

/**Entidad de persona contratada del Banco de Venezuela*/
public class Contratado {
	/**
	 * Tipo de Persona Contratada
	 * Campo en tabla 030 de BD: tipper_id
	 */
	String tipoPersona;
	
	/**
	 * Cedula del Contratado
	 * Campo en tabla 030 de BD: contratados_ced
	 */
	int cedula;
	
	/**
	 * Nombre del Contratado
	 * Campo en tabla 030 de BD: contratados_nombre
	 */
	String nombre;

	public int getCedula() {
		return cedula;
	}

	public void setCedula(int cedula) {
		this.cedula = cedula;
	}

	public String getNombre() {
		return nombre;
	}

	public void setNombre(String nombre) {
		this.nombre = nombre;
	}

	public String getTipoPersona() {
		return tipoPersona;
	}

	public void setTipoPersona(String tipoPersona) {
		this.tipoPersona = tipoPersona;
	}
}
