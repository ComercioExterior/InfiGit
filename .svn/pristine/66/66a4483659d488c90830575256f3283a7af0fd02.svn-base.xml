<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
     <field 
        name="client_id" 
        type="integer" 
        description="Listado Clientes "
        nullable="1"
        size="1" />
        
       <field 
        name="num_cheque" 
        type="varchar" 
        description="Numero del Cheque"
        nullable="1"
        size="25" />
 
        <field 
        name="fecha_desde" 
        type="varchar" 
        description="Fecha Desde"
        nullable="0"
        size="100" />
               
        <field 
        name="fecha_hasta" 
        type="varchar" 
        description="Fecha Hasta"
        nullable="0"
        size="100" />
        
        <field 
        name="formato_salida" 
        type="varchar" 
        description="Formato de Salida"
        nullable="1"
        size="100" />
    
</record>

