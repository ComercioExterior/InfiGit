<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->     
 	
        <field 
        name="ui_id" 
        type="integer" 
        description="Id de Unidad de Inversion"
        nullable="0"
        size="10" />
        
        <field 
        name="undinv_nombre" 
        type="varchar" 
        description="Nombre  de Unidad de Inversion"
        nullable="0"
        size="100" />
        
        <field 
        name="nro_jornada" 
        type="varchar" 
        description="Numero de jornada"
        nullable="1"
        size="20" />
        
        <field 
        name="parametro_bcv_online" 
        type="varchar" 
        description="Parametro BCV online"
        nullable="1"
        size="20" />
    
</record>

