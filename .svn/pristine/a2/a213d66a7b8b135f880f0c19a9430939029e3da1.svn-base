package com.bdv.infi.data;

public class TipoCarta {
	/*
	 * Identificador del Tipo de Carta
	 */
	private int idTipoCarta;
	
	/*
	 * Descripcion del Tipo de Carta
	 */
	private String cartaDescripcion;
	
	/*
	 * Nombre del Tipo de Carta
	 */
	private String cartaNombre;

	public String getCartaDescripcion() {
		return cartaDescripcion;
	}

	public void setCartaDescripcion(String cartaDescripcion) {
		this.cartaDescripcion = cartaDescripcion;
	}

	public String getCartaNombre() {
		return cartaNombre;
	}

	public void setCartaNombre(String cartaNombre) {
		this.cartaNombre = cartaNombre;
	}

	public int getIdTipoCarta() {
		return idTipoCarta;
	}

	public void setIdTipoCarta(int idTipoCarta) {
		this.idTipoCarta = idTipoCarta;
	}
}
