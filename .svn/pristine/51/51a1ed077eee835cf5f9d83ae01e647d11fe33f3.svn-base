BODY
{
    BACKGROUND-ATTACHMENT: fixed;
    Background-Position : Center Center ;
    Background-Repeat : No-Repeat ;
    BACKGROUND-COLOR: transparent;
    BACKGROUND-IMAGE: url(../images/infi_gn_fondo03.jpg);
    FONT-FAMILY: Arial;
    FONT-SIZE: 9pt;
    COLOR: #2A2A2A
}
INPUT {
	 FONT-SIZE: 8pt;
}

SELECT {
	 FONT-SIZE: 8pt;
}

TABLE.datatable
{
    BORDER-BOTTOM: #686868;
    BORDER-COLLAPSE: collapse;
    BORDER-LEFT: #686868;
    BORDER-RIGHT: #686868;
    BORDER-TOP: #686868;
    FONT-FAMILY: "Arial";
    FONT-SIZE: 8pt;
    TABLE-LAYOUT: fixed;  
}
TABLE.datatable TH
{
	a:link color:#444444;
    BACKGROUND-COLOR: #A5A4A5;
    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid;
    COLOR: white;   
    
    
}
/*Estilos de Links para los TH del estilo datatable*/
.TABLE.datatable TH a, .TABLE.datatable TH a:link{
color:#FFFFFF;
}	  

.TABLE.datatable TH a, .TABLE.datatable TH a:visited{
color:#FFFFFF;
}

.TABLE.datatable TH a, .TABLE.datatable TH a:active{
color:#FFFFFF;
}	  

.TABLE.datatable TH a, .TABLE.datatable TH a:hover{
color:#E7E7E7;
}

TABLE.datatable TD
{
	BACKGROUND-COLOR: #E7E7E7;
    BORDER-BOTTOM: 1px solid;
    BORDER-LEFT: 1px solid;
    BORDER-RIGHT: 1px solid;
    BORDER-TOP: 1px solid
}
