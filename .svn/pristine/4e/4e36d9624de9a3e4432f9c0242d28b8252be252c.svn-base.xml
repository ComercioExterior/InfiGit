	<queryString><![CDATA[SELECT
     ordene_usr_sucursal,
     infi_tb_201_ctes.CLIENT_NOMBRE,
     ordene_id,
     uniinv_id,
     to_char(ORDENE_PED_FE_VALOR,'dd-mm-yyyy HH24:mi:ss') as ORDENE_PED_FE_ORDEN,
     infi_tb_201_ctes.CLIENT_CEDRIF,
     (infi_tb_204_ordenes.ORDENE_PED_TOTAL - infi_tb_204_ordenes.ORDENE_PED_COMISIONES) as capital,
     infi_tb_204_ordenes.ORDENE_PED_TOTAL,
     infi_tb_204_ordenes.ORDENE_PED_COMISIONES,
     infi_tb_204_ordenes.ORDENE_PED_MONTO,
     infi_tb_203_ordenes_status.ORDSTA_NOMBRE,
     infi_tb_106_unidad_inversion.UNDINV_NOMBRE
FROM
     infi_tb_204_ordenes INNER JOIN infi_tb_201_ctes ON infi_tb_204_ordenes.CLIENT_ID = infi_tb_201_ctes.CLIENT_ID
     INNER JOIN infi_tb_203_ordenes_status ON infi_tb_204_ordenes.ORDSTA_ID = infi_tb_203_ordenes_status.ORDSTA_ID
     INNER JOIN INFI_TB_106_UNIDAD_INVERSION ON infi_tb_204_ordenes.UNIINV_ID = INFI_TB_106_UNIDAD_INVERSION.UNDINV_ID
WHERE
     ordene_usr_sucursal IS NOT null
     and infi_tb_204_ordenes.ORDSTA_ID in ('REGISTRADA','ENVIADA','ADJUDICADA','LIQUIDADA')
     $P!{p_filter}
GROUP BY
     ordene_usr_sucursal,
     infi_tb_201_ctes.CLIENT_NOMBRE,
     ordene_id,
     uniinv_id,
     ORDENE_PED_FE_VALOR,
     infi_tb_201_ctes.CLIENT_CEDRIF,
     infi_tb_204_ordenes.ORDENE_PED_TOTAL,
     infi_tb_204_ordenes.ORDENE_PED_COMISIONES,
     infi_tb_204_ordenes.ORDENE_PED_MONTO,
     infi_tb_203_ordenes_status.ORDSTA_NOMBRE,
     INFI_TB_106_UNIDAD_INVERSION.UNDINV_NOMBRE]]></queryString>

