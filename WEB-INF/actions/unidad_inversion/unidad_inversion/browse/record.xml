<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="nombreUnidadInversion" 
        type="varchar" 
        description="Nombre de la Unidad de Inversion"
        nullable="1"
        size="100" />
        
        <field 
        name="idInstrumentoFinanciero" 
        type="varchar" 
        description="Tipo de Instrumento"
        nullable="1"
        size="50" /> 
        
        <field 
        name="idEmpresaEmisor" 
        type="varchar" 
        description="Emisor"
        nullable="1"
        size="25" />   
        
        <field 
        name="filtroFechaEmisionDesde" 
        type="varchar" 
        description="Fecha de Emision Desde"
        nullable="1"
        size="10" /> 
        
        <field 
        name="filtroFechaEmisionHasta" 
        type="varchar" 
        description="Fecha de Emision Hasta"
        nullable="1"
        size="10" />                        
    
        <field 
        name="idUIStatus" 
        type="varchar" 
        description="Status de la Unidad de Inversion"
        nullable="1"
        size="50" />    
           
    
</record>

