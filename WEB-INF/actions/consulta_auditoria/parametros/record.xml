<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->

    	<field 
		name="action" 
		type="varchar" 
		description="Url" 
		nullable="0" 
		size="50" />
		
		<field 
		name="fecha" 
		type="varchar" 
		description="Fecha" 
		nullable="0" 
		size="50" />
		
		<field 
		name="time" 
		type="varchar" 
		description="Hora" 
		nullable="0" 
		size="50" />
		
		<field 
		name="ip" 
		type="varchar" 
		description="IP" 
		nullable="0" 
		size="50" />
		
		<field 
		name="usuario" 
		type="varchar" 
		description="Usuario" 
		nullable="0" 
		size="50" />
		
 </record>

