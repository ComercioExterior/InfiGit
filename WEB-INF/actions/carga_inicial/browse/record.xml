<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="fichero.tempfile" 
        type="varchar" 
        description="Archivo"
        nullable="1"
        size="100" />
        
        <field 
        name="fichero.filename" 
        type="varchar" 
        description="Nombre original del archivo"
        nullable="1"
        size="100" />
        
        <field 
        name="fichero.content-type" 
        type="varchar" 
        description="Content type del archivo descargado"
        nullable="1"
        size="100" />
               
         <field 
        name="num_archivo" 
        type="integer" 
        description="N&uacute;mero del archivo a leer"
        nullable="1"
        size="50" />
        
        
         <field 
        name="userid" 
        type="varchar" 
        description="Nombre de Usuario"
        nullable="1"
        size="80" />
        
               <field 
        name="cod_mapa" 
        type="varchar" 
        description="C&oacute;digo de mapa de archivo"
        nullable="1"
        size="80" />
        
 
    
</record>

