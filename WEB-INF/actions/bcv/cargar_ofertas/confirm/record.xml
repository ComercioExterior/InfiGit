        description="Fecha para carga de pactos diaria"
        nullable="0"
        size="50" />
        
        <field 
        name="undinv_id" 
        type="integer"  
        description="Unidad de Inversión"
        nullable="0"
        size="50" />
        
        <field 
        name="nro_jornada" 
        type="varchar" 
        description="Unidad de Inversión"
        nullable="1"
        size="50" />
    
</record>

