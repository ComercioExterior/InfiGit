<?xml version="1.0" encoding="ISO-8859-1"?>
<record>
    
    <!--
    
    NOTA:
    
    1) valores validos para el atributo 'type': 
       varchar, decimal, integer, date
    
    2) el atributo 'size' solo importa para campos
       del tipo varchar se ignora para otro tipo de dato
       pero DEBE estar definido aunque tenga un valor 0 (cero)
       y representa la longitud maxima del campo.
    
    -->
    
        <field 
        name="titulo_id" 
        type="varchar" 
        description="Titulos "
        nullable="1"
        size="100" />
  
        <field 
        name="client_id" 
        type="integer" 
        description="Cliente"
        nullable="1"
        size="50" />
        
        <field 
        name="mes" 
        type="integer" 
        description="Mes "
        nullable="0"
        size="2" />
        
        <field 
        name="anio" 
        type="varchar" 
        description="A&ntilde;o "
        nullable="1"
        size="100" />
        
       
           
</record>

